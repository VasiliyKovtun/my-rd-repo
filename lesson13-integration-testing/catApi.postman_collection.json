							"pm.test(\"Status code is 200\", function () {",
							"    pm.response.to.have.status(200);",
							"});",
							"",
							"pm.test(\"The image–favourites relation has been verified\", function () {",
							"    const responseJson = pm.response.json();",
							"    const savedImage = JSON.parse(pm.collectionVariables.get(\"imageObject\"));",
							"",
							"    pm.expect(responseJson).to.be.not.null;",
							"    pm.expect(responseJson.image_id).to.equal(pm.variables.get(\"image_id\"));",
							"    pm.expect(responseJson.sub_id).to.equal(pm.variables.get(\"sub_id\"));",
							"    pm.expect(responseJson.image.url).to.equal(savedImage.url);",
							"    pm.expect(responseJson.image.id).to.equal(savedImage.id);",
							"});"
						],
